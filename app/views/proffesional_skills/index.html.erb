<p id="notice"><%= notice %></p>

<h1>Proffesional Skills</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @proffesional_skills.each do |proffesional_skill| %>
      <tr>
        <td><%= link_to 'Show', proffesional_skill %></td>
        <td><%= link_to 'Edit', edit_proffesional_skill_path(proffesional_skill) %></td>
        <td><%= link_to 'Destroy', proffesional_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

